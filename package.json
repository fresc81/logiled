{
  "name": "logiled",
  "version": "1.0.0",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/fresc81/logiled.git"
  },
  "keywords": [
    "logitech",
    "gaming",
    "keyboard",
    "LED",
    "Logitech Gaming Software"
  ],
  "scripts": {
    "download": "node ./download-sdk",
    "configure": "node-gyp configure",
    "build": "node-gyp build",
    "clean": "node-gyp clean",
    "rebuild": "node-gyp rebuild",
    "dist": "npm run-script compile && prebuild --all --strip --arch x64 && prebuild --all --strip --arch ia32",
    "compile": "tsc",
    "install": "prebuild-install || node-gyp --release rebuild || echo Please build binaries manually by running `npm install` within the module directory.",
    "test": "mocha --require ts-node/register src/*.spec.ts"
  },
  "devDependencies": {
    "@types/assert": "^1.4.0",
    "@types/async": "^2.0.50",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "async": "^2.1.5",
    "fstream": "^1.0.11",
    "mocha": "^5.2.0",
    "nan": "^2.0.5",
    "node-gyp": "^3.8.0",
    "nw-gyp": "^3.4.0",
    "prebuild": "^6.1.0",
    "progress-bar": "^0.1.1",
    "request": "^2.81.0",
    "request-progress": "^3.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^2.2.1",
    "unzip": "^0.1.11"
  },
  "dependencies": {
    "bindings": "^1.2.1",
    "prebuild-install": "^2.1.1"
  },
  "license": "BSD-2-Clause"
}
